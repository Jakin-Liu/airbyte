{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": ["null", "string"]
    },
    "handler": {
      "type": ["null", "string"]
    },
    "timestamp": {
      "type": "string",
      "format": "date-time",
      "airbyte_type": "timestamp_with_timezone"
    },
    "description": {
      "type": ["null", "string"]
    },
    "profile_image_url": {
      "type": ["null", "string"]
    },
    "followers_count": {
      "type": ["null", "integer"]
    },
    "following_count": {
      "type": ["null", "integer"]
    },
    "tweet_count": {
      "type": ["null", "integer"]
    },
    "listed_count": {
      "type": ["null", "integer"]
    },
    "name": {
      "type": ["null", "string"]
    }
  }
}
