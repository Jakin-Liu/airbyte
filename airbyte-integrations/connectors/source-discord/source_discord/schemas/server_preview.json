      "type": ["null", "string"]
    },
    "name": {
      "type": ["null", "string"]
    },
    "icon": {
      "type": ["null", "string"]
    },
    "description": {
      "type": ["null", "string"]
    },
    "home_header": {
      "type": ["null", "string"]
    },
    "splash": {
      "type": ["null", "string"]
    },
